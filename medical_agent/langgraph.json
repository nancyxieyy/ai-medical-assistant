{
  "$schema": "https://langgra.ph/schema.json",
  "dependencies": ["."],
  "graphs": {
    "medical_rag_graph": {
      "id": "medical_rag_graph",
      "nodes": [
        "asr_node",
        "jina_embedding_node",
        "rag_query_node",
        "llm_doctor_node"
      ],
      "edges": [
        ["asr_node", "jina_embedding_node"],
        ["jina_embedding_node", "rag_query_node"],
        ["rag_query_node", "llm_doctor_node"]
      ]
    },
    "agent": "./src/agent/graph.py:graph"
  },
  "env": ".env",
  "image_distro": "wolfi",
  "nodes": [
    {
      "id": "jina_embedding_node",
      "type": "python",
      "path": "../backend/nodes/embedding_node.py",
      "class_name": "JinaEmbeddingNode",
      "inputs": ["texts"],
      "outputs": ["embeddings"]
    },
    {
      "id": "rag_query_node",
      "type": "python",
      "path": "../backend/nodes/rag_query_node.py",
      "class_name": "RAGQueryNode",
      "inputs": ["embeddings"],
      "outputs": ["context"]
    },
    {
      "id": "llm_doctor_node",
      "type": "python",
      "path": "../backend/nodes/llm_doctor_node.py",
      "class_name": "LLMDoctorAdviceNode",
      "inputs": ["mode", "transcript", "context"],
      "outputs": ["llm_output"]
    }
  ]
}